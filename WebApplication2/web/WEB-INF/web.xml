<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>establishconnection</servlet-name>
        <servlet-class>establishconnection</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.WebApplication2.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>add_at_database</servlet-name>
        <servlet-class>add_at_database</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_at_database</servlet-name>
        <servlet-class>delete_at_database</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>replace_at_database</servlet-name>
        <servlet-class>replace_at_database</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>establishconnection</servlet-name>
        <url-pattern>/establishconnection</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>add_at_database</servlet-name>
        <url-pattern>/add_at_database</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_at_database</servlet-name>
        <url-pattern>/delete_at_database</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>replace_at_database</servlet-name>
        <url-pattern>/replace_at_database</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
